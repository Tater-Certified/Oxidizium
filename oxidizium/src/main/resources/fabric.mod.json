{
  "schemaVersion": 1,
  "id": "${modid}",
  "version": "${version}",
  "name": "${mod}",
  "description": "Implementing Minecraft In Rust",
  "authors": [],
  "contact": {},
  "license": "MIT",
  "icon": "assets/oxidizium/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.github.tatercertified.oxidizium.Oxidizium"
    ],
    "mixinsquared": [
      "com.github.tatercertified.oxidizium.compat.DebugifyMixinCanceller"
    ]
  },
  "mixins": [
    "oxidizium.mixins.json"
  ],
  "depends": {
    "fabricloader": "*",
    "minecraft": ">=${minecraft_version}",
    "java": "${java_ver}"
  },
  "custom": {
    "lithium:options": {
      "math.sine_lut": false
    }
  }
}